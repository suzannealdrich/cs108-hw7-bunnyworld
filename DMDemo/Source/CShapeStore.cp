// CShapeStore.cp#include "CShapeStore.h"#include "DM.h"CShapeStore::CShapeStore()	: Store() {	SetString(TYPE, SHAPE);}void CShapeStore::getType(StringPtr outType) {	Boolean found = FindString(TYPE, outType);}void CShapeStore::setName(ConstStringPtr name) {	SetString(NAME, name);}void CShapeStore::getName(StringPtr outName) {	Boolean found = FindString(NAME, outName);}void CShapeStore::setPage(Int32 pageNum) {	SetInt32(PAGENUM, pageNum);}void CShapeStore::getPage(Int32& outPageNum) {	Boolean found = FindInt32(PAGENUM, outPageNum);}void CShapeStore::setStart(Point point) {	SetInt16(STARTH, point.h);	SetInt16(STARTV, point.v);}	void CShapeStore::getStart(Point& outPoint) {	Boolean foundh = FindInt16(STARTH, outPoint.h);	Boolean foundv = FindInt16(STARTV, outPoint.v);}void CShapeStore::setEnd(Point point) {	SetInt16(ENDH, point.h);	SetInt16(ENDV, point.v);}void CShapeStore::getEnd(Point& outPoint) {	Boolean foundh = FindInt16(ENDH, outPoint.h);	Boolean foundv = FindInt16(ENDV, outPoint.v);}void CShapeStore::setMoveable(Boolean isMoveable) {	SetInt32(MOVEABLE, (isMoveable? 1 : 0));}void CShapeStore::getMoveable(Boolean& isMoveable) {	Int32 moves;	Boolean found = FindInt32(MOVEABLE, moves);	isMoveable = (moves? true : false);}void CShapeStore::setVisible(Boolean isVisible) {	SetInt32(VISIBLE, (isVisible? 1 : 0));}void CShapeStore::getVisible(Boolean& isVisible) {	Int32 shows;	Boolean found = FindInt32(VISIBLE, shows);	isVisible = (shows? true : false);}void CShapeStore::setScript(ConstStringPtr script) {	SetString(SCRIPT, script);}void CShapeStore::getScript(StringPtr outScript)	{	Boolean found = FindString(SCRIPT, outScript);}void CShapeStore::setPict(ConstStringPtr pictName) {	SetString(PICT, pictName);}void CShapeStore::getPict(StringPtr outPictName) {	Boolean found = FindString(PICT, outPictName);}