pt) {	mData->Soil(true);	SetString(SCRIPT, script);}void CShapeStore::getScript(StringPtr outScript)	{	Boolean found = FindString(SCRIPT, outScript);}void CShapeStore::setPict(ConstStringPtr pictName) {	mData->Soil(true);	SetString(kPICT, pictName);}void CShapeStore::getPict(StringPtr outPictName) {	Boolean found = FindString(kPICT, outPictName);}