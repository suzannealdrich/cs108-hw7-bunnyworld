rame(RGBColor frameColor) {	SetInt16(FRAMER, frameColor.red);	SetInt16(FRAMEG, frameColor.green);	SetInt16(FRAMEB, frameColor.blue);}void CShapeStore::getFrame(RGBColor& frameColor) {	Boolean foundr = FindInt16(FRAMER, frameColor.red);	Boolean foundg = FindInt16(FRAMEG, frameColor.green);	Boolean foundb = FindInt16(FRAMEB, frameColor.blue);}void CShapeStore::setFill(RGBColor fillColor) {	SetInt16(FILLR, fillColor.red);	SetInt16(FILLG, fillColor.green);	SetInt16(FILLB, fillColor.blue);}void CShapeStore::getFill(RGBColor& fillColor) {	Boolean foundr = FindInt16(FILLR, fillColor.red);	Boolean foundg = FindInt16(FILLG, fillColor.green);	Boolean foundb = FindInt16(FILLB, fillColor.blue);}	